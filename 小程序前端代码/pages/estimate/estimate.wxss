.estimateContain{
    height: 100vh;
    width: 100vw;
    display: flex;
    flex-direction: column;
    text-align: center;
    align-items: center;
    background: #f5f5f5;
}

.words{
    background: white;
    margin-top: 20vh;
    height: 100rpx;
    line-height: 100rpx;
    width: 350rpx;
    border-radius: 30rpx;
}

.choose{
    width: 100%;
    margin-top: 20vh;
    display: flex;
    flex-direction: row;
}

.choose button{
    width: 40%;
}

.score{
    background: white;
    margin-top: 20vh;
    height: 200rpx;
    line-height: 100rpx;
    width: 350rpx;
    border-radius: 30rpx;
}