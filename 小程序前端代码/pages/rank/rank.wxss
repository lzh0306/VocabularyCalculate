.rankContainer {
    height: 100vh;
    width: 100vw;
}

.nav {
    width: 100%;
    height: 100rpx;
    display: flex;
    justify-content: space-around;
    align-items: center;
}

.swiper {
    height: 100%;
    width: 100%;
}

.listContain {
    margin-top: 20rpx;
    height: 100rpx;
    width: 80%;
    margin-left: 10%;
    border-radius: 20rpx;
    background: chartreuse;
    display: flex;
    flex-direction: row;
    line-height: 100rpx;
}

.name {
    line-height: 50rpx;
    display: flex;
    flex-direction: column;
}

.listContain image {
    margin: 10rpx;
    height: 80rpx;
    width: 80rpx;
}